<br>
<h5>
    <b>
        CHAT MESSAGES
    </b>
</h5>
<br><br>
<ul>
    {{#each chat}}
        <li class="container-fluid">
            <div class="row">
                {{#if this.photo}}
                    <div class="col-5 col-sm-4 col-md-3 col-lg-2">
                        <img src="../{{this.photo}}" alt="profile pics" class="rounded-circle profile" />
                    </div>
                {{else}}
                    <div class="col-5 col-sm-4 col-md-3 col-lg-2">
                        <img src="../images/psylogo4.jpg" alt="profile pics" class="rounded-circle profile" />
                    </div>
                {{/if}}
                <div class="col-7 col-sm-8 col-md-9 col-lg-10">
                    <p><a href="/studentstudentprofile/{{this.id}}">{{this.surname}} {{this.firstname}}</a></p>
                    <p>
                        <b>Gender: </b>{{this.gender}}<br>
                        <b>Status: </b>{{this.bio}}
                    </p>
                </div>
            </div>
        </li>
        <br><br>
        {{else}}
            <p>THERE ARE NO CHAT MESSAGES</p>
    {{/each}}
</ul>
<br><br>