<h5><b>
    PROJECT TOPICS <span class="badge badge-danger badge-pill spany">{{length projectss}}</span>
  </b></h5>
<div class="container-fluid">
  <div class="row">
    <div class="col-12 dropdown dropright">
      <button class="btn btn-secondary btn-sm dropdown" data-toggle="dropdown"><span class="fa fa-search"></span>
        search</button>
      <div class="dropdown-menu dropdown-menu-right">
        <a class="dropdown-item" href="/studentgetprojecttopics">ALL</a>
        <a class="dropdown-item" href="/studentgetproject/amnesia">AMNESIA</a>
        <a class="dropdown-item" href="/studentgetproject/anxiety">ANXIETY</a>
        <a class="dropdown-item" href="/studentgetproject/depression">DEPRESSION</a>
        <a class="dropdown-item" href="/studentgetproject/dementia">DEMENTIA</a>
        <a class="dropdown-item" href="/studentgetproject/developmental_psychology">DEVELOPMENTAL PSYCHOLOGY</a>
        <a class="dropdown-item" href="/studentgetproject/educational_performance">EDUCATIONAL PERFORMANCE</a>
        <a class="dropdown-item" href="/studentgetproject/environment">ENVIRONMENT</a>
        <a class="dropdown-item" href="/studentgetproject/locus_of_control">LOCUS OF CONTROL</a>
        <a class="dropdown-item" href="/studentgetproject/personality">PERSONALITY</a>
        <a class="dropdown-item" href="/studentgetproject/physical_performance">PHYSICAL PERFORMANCE</a>
        <a class="dropdown-item" href="/studentgetproject/psychoactive_drugs">PSYCHOACTIVE DRUGS</a>
        <a class="dropdown-item" href="/studentgetproject/psychopathology">PSYCHOPATHOLOGY</a>
        <a class="dropdown-item" href="/studentgetproject/psychotherapy">PSYCHO THERAPY</a>
        <a class="dropdown-item" href="/studentgetproject/psychosis">PSYCHOSIS</a>
        <a class="dropdown-item" href="/studentgetproject/schizophrenia">SCHIZOPHRENIA</a>
        <a class="dropdown-item" href="/studentgetproject/school_of_thought">SCHOOL OF THOUGHT</a>
      </div>
    </div>
  </div>
</div>
<br><br>
<ol>
  <div class="container-fluid">
    <div class="row">
      {{#each projectss}}
        <li>
          <div class="col-12">
            <p><a href="/{{this.project}}">{{this.topic}}</a></p>
            <br>
          </div>
          <div class="col-12">
            <p>{{this.description}}</p>
          </div>
          <br><br>
        </li>
      {{else}}
        <p>THERE ARE NO UPLOADED PROJECT TOPIC ON THIS WEBSITE</p>
      {{/each}}
    </div>
  </div>
</ol>
<br><br>