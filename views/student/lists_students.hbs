<br>
<h2><b>
        <center>

            {{head}}
        </center>
    </b>
</h2>
<div class="dropdown" style="float:right">
    <button class="btn btn-secondary dropdown-toggle" data-toggle="dropdown">search</button>
    <div class="dropdown-menu">
        <a class="dropdown-item" href="/student100mates">100 LEVEL</a>
        <a class="dropdown-item" href="/student200mates">200 LEVEL</a>
        <a class="dropdown-item" href="/student300mates">300 LEVEL</a>
        <a class="dropdown-item" href="/student400mates">400 LEVEL</a>
    </div>
</div>
<br><br>
<ul>
    {{#each slow}}
        <li class="container-fluid">
            <div class="row">
                {{#if this.photo}}
                    <div class="col-5 col-sm-4 col-md-3 col-lg-2">
                        <img src="/{{this.photo}}" alt="image" class="rounded-circle" width="100%" height="130" />
                    </div>
                {{else}}
                    <div class="col-5 col-sm-4 col-md-3 col-lg-2">
                        <img src="../images/psylogo4.jpg" alt="image" class="rounded-circle" width="100%" height="130" />
                    </div>
                {{/if}}
                <div class="col-7 col-sm-8 col-md-9 col-lg-10">
                    <p><a href="/studentstudentprofile/{{this.id}}">{{this.surname}} {{this.firstname}}</a></p>
                    <p>
                        <b>GENDER: </b>{{this.gender}}<br>
                        <b>STATUS: </b>{{this.bio}}
                    </p>
                </div>
            </div>
        </li>
        <br><br>
        {{else}}
            <p>THERE ARE NO REGISTERED PSYCHOLOGY STUDENT</p>
    {{/each}}
</ul>
<br><br>