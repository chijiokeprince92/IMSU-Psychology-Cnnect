<br>
<h5>
  <b>

    {{head}}
  </b>
</h5>
<div class="container-fluid">
  <div class="row">
    <div class="col-12 dropdown dropright">
      <button class="btn btn-secondary btn-sm dropdown" data-toggle="dropdown"><span class="fa fa-search"></span>
        search</button>
      <div class="dropdown-menu dropdown-menu-right">
        <a class="dropdown-item" href="/studentlevelmates">ALL</a>
        <a class="dropdown-item" href="/studentmates/100">100 LEVEL</a>
        <a class="dropdown-item" href="/studentmates/200">200 LEVEL</a>
        <a class="dropdown-item" href="/studentmates/300">300 LEVEL</a>
        <a class="dropdown-item" href="/studentmates/400">400 LEVEL</a>
      </div>
    </div>
  </div>
</div>
<br><br>
<div class="container-fluid">
  <div class="row">
    <div class="col-12 col-md-9">
      <ul class="list-group list-group-flush">
        {{#each slow}}
          <li class="list-group-item">
            <div class="row">
              {{#if this.photo.url}}
                <div class="col-4 col-sm-2">
                  <img src="{{this.photo.url}}" alt="profile pics" class="profile" />
                </div>
              {{else}}
                <div class="col-4 col-sm-2">
                  <img src="/images/psylogo4.jpg" alt="profile pics" class="profile" />
                </div>
              {{/if}}
              <div class="col-8 col-sm-10">
                <p>{{#isEqual this.id}}{{this.surname}} {{this.firstname}}
        {{else}}<a href="/studentstudentprofile/{{this.id}}">{{this.surname}}
            {{this.firstname}} </a>{{/isEqual}}{{#if this.is_courserep}} <span class="fa fa-star-o"
                      style="font-size:20px;color:red"></span>{{/if}}
                </p>
                <p>
                  <b><span class="fa fa-user"></span> </b>{{this.gender}}<br>
                  <b><span class="fa fa-comment-o"></span> </b>{{this.bio}}
                </p>
              </div>
            </div>
          </li>
          <br><br>
        {{else}}
          <p>THERE ARE NO REGISTERED PSYCHOLOGY STUDENT</p>
        {{/each}}
      </ul>
      <br><br>
    </div>
  </div>
</div>