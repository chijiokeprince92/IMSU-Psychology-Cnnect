<br>
<h5><b>
    {{levy}} LEVEL COURSES
  </b>
</h5>
<div class="container-fluid">
  <div class="row">
    <div class="col-12 dropdown dropright">
      <button class="btn btn-secondary btn-block btn-sm dropdown" data-toggle="dropdown"><span
          class="fa fa-search"></span>
        search</button>
      <div class="dropdown-menu dropdown-menu-right">
        <a class="dropdown-item" href="/studentgetcourse/100">100 LEVEL</a>
        <a class="dropdown-item" href="/studentgetcourse/200">200 LEVEL</a>
        <a class="dropdown-item" href="/studentgetcourse/300">300 LEVEL</a>
        <a class="dropdown-item" href="/studentgetcourse/400">400 LEVEL</a>
      </div>
    </div>
  </div>
</div>
<br>
<h6><b>
    FIRST SEMESTER COURSES
  </b>
</h6>
<div class="container-fluid">
  <div class="row">
    <nav class="col-12">
      <div class="table-responsive-sm">
        <table class="table table-hover table-bordered">
          <thead class="thead-dark">
            <tr>
              <th>COURSE CODE</th>
              <th>COURSE TITLE</th>
              <th class="seek">LEVEL</th>
              <th class="seek">SEMESTER</th>
              <th class="seek">UNIT/S</th>
              <th>COURSE OUTLINE</th>
            </tr>
          </thead>
          {{#each first}}
            <tr>
              <td>{{this.coursecode}}</td>
              <td>{{this.coursetitle}}</td>
              <td class="seek">{{this.level}}</td>
              <td class="seek">{{this.semester}}st</td>
              <td class="seek">{{this.units}}</td>
              <td><a href="/studentviewcourse/{{this.id}}">view course</a></td>
            </tr>
          {{else}}
            <p>THERE ARE NO REGISTERED FIRST SEMESTER COURSES</p>
          {{/each}}
        </table>
        <br>
        <h6>
          ELECTIVE COURSES: {{elective1}}
        </h6>
        <table class="table table-hover table-bordered">
          {{#each first_borrowed}}
            <tr>
              <td>{{this.coursecode}}</td>
              <td>{{this.coursetitle}}</td>
              <td class="seek">{{this.level}}</td>
              <td class="seek">{{this.semester}}st</td>
              <td class="seek">{{this.units}}</td>
              <td><a href="/studentviewcourse/{{this.id}}">view course</a></td>
            </tr>
          {{else}}
            <p>THERE ARE NO REGISTERED BORROWED COURSES</p>
          {{/each}}
        </table>
      </div>
    </nav>
  </div>
</div>
<br><br>
