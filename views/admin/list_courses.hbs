<!DOCTYPE html>
<html lang="en-US">

<head>
    {{>head}}
</head>

<body>
    {{>admin_lecbody}}
    <br>
    <br>
    <h2><b><center>{{levy}} LEVEL COURSES</center></b></h2>
    <br>
    <h3><b><center>FIRST SEMESTER COURSES</center></b></h3>
    <div class="container-fluid">
        <div class="row">
            <nav class="col-12">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead class="thead-dark">
                            <tr>
                                <th>COURSE CODE</th>
                                <th>COURSE TITLE</th>
                                <th>LEVEL</th>
                                <th>SEMESTER</th>
                                <th>UNIT/S</th>
                                <th>COURSE OUTLINE</th>
                            </tr>
                        </thead>
                        {{#each first}}
                            <tr>
                                <td>{{this.coursecode}}</td>
                                <td>{{this.coursetitle}}</td>
                                <td>{{this.level}}</td>
                                <td>{{this.semester}}st</td>
                                <td>{{this.units}}</td>
                                <td><a href="/adminviewcourse/{{this.id}}">view course</a></td>
                            </tr>
                            {{else}}
                                <p>THERE ARE NO REGISTERED FIRST SEMESTER COURSES</p>
                        {{/each}}
                    </table>
                    <br>
                    <h3>
                        <center>ELECTIVE COURSES: {{elective1}}</center>
                    </h3>
                    <table class="table table-hover">
                        <thead class="thead-light">
                            <tr>
                                <th>COURSE CODE</th>
                                <th>COURSE TITLE</th>
                                <th>LEVEL</th>
                                <th>SEMESTER</th>
                                <th>UNIT/S</th>
                                <th>COURSE OUTLINE</th>
                            </tr>
                        </thead>
                        {{#each first_borrowed}}
                            <tr>
                                <td>{{this.coursecode}}</td>
                                <td>{{this.coursetitle}}</td>
                                <td>{{this.level}}</td>
                                <td>{{this.semester}}st</td>
                                <td>{{this.units}}</td>
                                <td><a href="/adminviewcourse/{{this.id}}">view course</a></td>
                            </tr>
                            {{else}}
                                <p>THERE ARE NO REGISTERED BORROWED COURSES</p>
                        {{/each}}
                    </table>
                </div>
            </nav>
        </div>
    </div>
    <br>
    <br>
    <h3><b><center>SECOND SEMESTER COURSES</center></b></h3>
    <div class="container-fluid">
        <div class="row">
            <nav class="col-sm-12">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead class="thead-dark">
                            <tr>
                                <th>COURSE CODE</th>
                                <th>COURSE TITLE</th>
                                <th>LEVEL</th>
                                <th>SEMESTER</th>
                                <th>UNIT/S</th>
                                <th>COURSE OUTLINE</th>
                            </tr>
                        </thead>
                        {{#each second}}
                            <tr>
                                <td>{{this.coursecode}}</td>
                                <td>{{this.coursetitle}}</td>
                                <td>{{this.level}}</td>
                                <td>{{this.semester}}nd</td>
                                <td>{{this.units}}</td>
                                <td><a href="/adminviewcourse/{{this.id}}">view course</a></td>
                            </tr>
                            {{else}}
                                <p>THERE ARE NO REGISTERED SECOND SEMESTER COURSES</p>
                        {{/each}}
                    </table>
                    <br>
                    <h3>
                        <center>ELECTIVE COURSES: {{elective2}}</center>
                    </h3>
                    <table class="table table-hover">
                        <thead class="thead-light">
                            <tr>
                                <th>COURSE CODE</th>
                                <th>COURSE TITLE</th>
                                <th>LEVEL</th>
                                <th>SEMESTER</th>
                                <th>UNIT/S</th>
                                <th>COURSE OUTLINE</th>
                            </tr>
                        </thead>
                        {{#each second_borrowed}}
                            <tr>
                                <td>{{this.coursecode}}</td>
                                <td>{{this.coursetitle}}</td>
                                <td>{{this.level}}</td>
                                <td>{{this.semester}}nd</td>
                                <td>{{this.units}}</td>
                                <td><a href="/adminviewcourse/{{this.id}}">view course</a></td>
                            </tr>
                            {{else}}
                                <p>THERE ARE NO REGISTERED BORROWED COURSES</p>
                        {{/each}}
                    </table>
                </div>
            </nav>
        </div>
    </div>
    <br><br>
    <footer>{{>footer}}</footer>
</body>

</html>